    "dev:iot": "npm run build:iot -- --watch",
    "dev:lark": "npm run build:lark -- --watch",
    "dev:kwai": "npm run build:kwai -- --watch",
    "dev:xhs": "npm run build:xhs -- --watch",
    "e2e:h5": "zx ./scripts/h5.e2e.mjs",
    "e2e:weapp": "zx ./scripts/weapp.e2e.mjs",
    "cy:open": "cypress open",
    "cy:run": "cypress run --e2e --headless"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
