{
  "name": "taro-plugin-style-resource",
  "version": "0.1.8",
  "description": "Taro 构建编译插件,给less或stylus添加公共样式资源",
  "keywords": [
    "Taro",
    "style",
    "less",
    "stylus"
  ],
  "main": "index.js",
  "typings": "types/index.d.ts",
  "files": [
    "dist",
    "index.js",
    "types"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w",
    "clean": "rimraf dist",
    "release": "npm run clean && npm run build && release-it",
    "release:try": "npm run release -- --dry-run",
    "example": "pnpm --filter=./examples/* --workspace-concurrency=1 "
  },
  "author": "Binbiubiubiu <2632003786@qq.com>",
  "license": "MIT",
  "dependencies": {
    "style-resources-loader": "^1.5.0"
  },
  "devDependencies": {
    "@hapi/joi": "^17.1.1",
    "@release-it/conventional-changelog": "^4.3.0",
    "@tarojs/service": "^3.5.7",
    "release-it": "^14.14.3",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.4",
    "webpack-chain": "^6.5.1"
  },
  "repository": "git+https://github.com/Binbiubiubiu/taro-plugin-style-resource.git",
  "bugs": {
    "url": "https://github.com/Binbiubiubiu/taro-plugin-style-resource/issues"
  },
  "homepage": "https://github.com/Binbiubiubiu/taro-plugin-style-resource#readme",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
